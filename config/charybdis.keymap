				&kp N0  					&kp N1   	&kp N2   	&kp N3   	&kp N4     	&kp N5   	&kp N6		&kp N7   	&kp N8   	&kp N9   	&kp N0   			&kp KP_EQUAL
				&trans/*LOCKING_NUM*/&trans      &trans      &trans      &trans      &trans      &kp DOT     &kp N4   	&kp N5   	&kp N6   	&kp KP_MINUS   	&kp KP_PLUS
				&trans/*SCROLLLOCK*/ &trans      &trans      &trans      &trans      &trans      &kp N0 		&kp N1   	&kp N2   	&kp N3   	&kp KP_DIVIDE  	&kp KP_MULTIPLY
															&trans      &trans      &trans      &trans			&trans
																		&none       &none       &none
			>;
			trackball-bindings = <&tmv_coarse>;
		};

